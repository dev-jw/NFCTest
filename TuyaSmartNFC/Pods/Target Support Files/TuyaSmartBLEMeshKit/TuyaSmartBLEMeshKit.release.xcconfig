CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES = YES
CLANG_WARN_QUOTED_INCLUDE_IN_FRAMEWORK_HEADER = NO
CONFIGURATION_BUILD_DIR = ${PODS_CONFIGURATION_BUILD_DIR}/TuyaSmartBLEMeshKit
DEFINES_MODULE = YES
FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/TuyaSmartNetworkKit/ios" "${PODS_ROOT}/TuyaSmartQUIC/Carthage/Build/iOS"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Private" "${PODS_ROOT}/Headers/Private/TuyaSmartBLEMeshKit" "${PODS_ROOT}/Headers/Public"
OTHER_CFLAGS = $(inherited) -fmodule-map-file="${PODS_ROOT}/Headers/Private/TuyaSmartBLEKit/TuyaSmartBLEKit.modulemap" -fmodule-map-file="${PODS_ROOT}/Headers/Private/TuyaSmartBaseKit/TuyaSmartBaseKit.modulemap" -fmodule-map-file="${PODS_ROOT}/Headers/Private/TuyaSmartDeviceCoreKit/TuyaSmartDeviceCoreKit.modulemap" -fmodule-map-file="${PODS_ROOT}/Headers/Private/TuyaSmartMQTTChannelKit/TuyaSmartMQTTChannelKit.modulemap" -fmodule-map-file="${PODS_ROOT}/Headers/Private/TuyaSmartSocketChannelKit/TuyaSmartSocketChannelKit.modulemap" -fmodule-map-file="${PODS_ROOT}/Headers/Private/TuyaSmartUtil/TuyaSmartUtil.modulemap"
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}
PODS_TARGET_SRCROOT = ${PODS_ROOT}/../../../TuyaSmartSDK/TuyaSmartBLEMeshKit
PODS_XCFRAMEWORKS_BUILD_DIR = $(PODS_CONFIGURATION_BUILD_DIR)/XCFrameworkIntermediates
PRODUCT_BUNDLE_IDENTIFIER = org.cocoapods.${PRODUCT_NAME:rfc1034identifier}
SKIP_INSTALL = YES
TUYA_PRIVACY_USAGE_DESCRIPTION = NSBluetoothAlwaysUsageDescription NSBluetoothPeripheralUsageDescription
USE_RECURSIVE_SCRIPT_INPUTS_IN_SCRIPT_PHASES = YES
